//打印从1到122 每六个换一行  *hint:for里套for

int main( )
{
  int i = 0;
  int j = 0;

  for (i = 0; i < 122; i++)
  {
    printf("%6d", i+1);

    j++;

     	if (j%6 == 0){
  		printf("\n");
  	}
  }
  
  return 0;
 }
 
 
 
